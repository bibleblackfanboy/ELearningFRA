<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" stylesheets="@steyles5.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.elearningfra.ModulAdminController">
    <TableView fx:id="moduleTable" AnchorPane.topAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.bottomAnchor="70.0" styleClass="table-view">
        <columns>
            <TableColumn fx:id="idColumn" text="ID" />
            <TableColumn fx:id="nameColumn" text="Name" />
            <TableColumn fx:id="deleteColumn" text="Delete" />
            <TableColumn fx:id="editColumn" text="Edit" />
        </columns>
    </TableView>
    <Button fx:id="addButton" onAction="#handleAddButton" text="Add" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0" />
    <Button mnemonicParsing="false" onAction="#back" prefHeight="42.0" prefWidth="75.0" text="Back" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="90.0" />
</AnchorPane>
